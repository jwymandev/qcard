{
  "name": "qcard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.17.0"
  },
  "scripts": {
    "dev": "next dev -p 3002",
    "build": "NEXT_BUILD_SKIP_DB=true npx prisma generate && NODE_ENV=production ESLINT_NO_DEV_ERRORS=true next build",
    "build:do": "NEXT_BUILD_SKIP_DB=true NEXT_PRIVATE_STANDALONE=1 NEXT_TELEMETRY_DISABLED=1 npx prisma generate && NODE_ENV=production ESLINT_NO_DEV_ERRORS=true next build",
    "build:production": "NODE_ENV=production npx prisma migrate deploy && npm run build",
    "deploy": "npx prisma migrate deploy && npm run build",
    "start": "node production-start.js",
    "lint": "next lint",
    "postinstall": "npx prisma generate",
    "setup-uploads": "node scripts/init-uploads.js",
    "predev": "npm run setup-uploads",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run build",
    "check-auth": "node scripts/check-auth-config.js || true",
    "clean-build": "rm -rf .next && npm run build",
    "db:migrate": "npx prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:push": "npx prisma db push",
    "db:check-tables": "node scripts/check-db-tables.js",
    "db:reset": "node scripts/ssl-fix-reset-db.js",
    "db:verify": "node scripts/verify-db-connection.js",
    "db:verify-deployment": "node scripts/verify-deployment-db.js",
    "db:migrate-schema": "node scripts/migrate-profile-schema.js",
    "db:setup-local": "node scripts/setup-local-postgres.js",
    "db:ssl-fix": "node scripts/fix-ssl-certificate.js",
    "db:fix-studio": "node scripts/check-and-fix-studio.js",
    "db:init-all-studios": "node scripts/studio-auto-initializer.js",
    "db:startup-fix": "node scripts/startup-fix.js",
    "db:auto-init": "node scripts/db-initialize-auto.js",
    "db:initialize-deployment": "node scripts/initialize-deployment.js",
    "stripe:setup-plans": "node scripts/setup-stripe-plans.js",
    "stripe:test-subscription": "node scripts/test-subscription-flow.js",
    "admin:check": "node scripts/check-admin-user.js",
    "user:clear-sessions": "node scripts/clear-user-sessions.js",
    "user:fix-tenant": "node scripts/fix-user-tenant.js",
    "studio:init": "bash scripts/init-visit.sh",
    "predeploy": "npx prisma migrate deploy",
    "do:deploy": "npx prisma migrate deploy && npx prisma generate && next build",
    "do:fix": "node scripts/ssl-fix-reset-db.js && npm run build",
    "do:fix-projects": "node scripts/do-deployment-fix.js",
    "do:push": "npx prisma db push && npm run build",
    "do:first-deploy": "node scripts/first-deploy.js",
    "do:deploy-with-schema": "node scripts/first-deploy.js && npm run build",
    "do:deploy-full": "NEXT_BUILD_SKIP_DB=true NEXT_PRIVATE_STANDALONE=1 NEXT_PUBLIC_SKIP_API_ROUTES=1 NODE_OPTIONS=\"--max_old_space_size=4096\" node scripts/do-deploy.js",
    "do:build-only": "NEXT_BUILD_SKIP_DB=true NEXT_PRIVATE_STANDALONE=1 NEXT_PUBLIC_SKIP_API_ROUTES=1 node scripts/do-deploy.js --skip-migration",
    "do:prepare-deploy": "node scripts/do-full-deployment.js",
    "do:healthcheck": "node scripts/production-healthcheck.js --remote",
    "healthcheck": "node scripts/production-healthcheck.js",
    "healthcheck:remote": "node scripts/production-healthcheck.js --remote",
    "deploy:prod": "npm run typecheck && npm run lint && npm run build && npm run healthcheck"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^5.14.0",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^5.6.0",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.356.0",
    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.27",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-big-calendar": "^1.18.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.56.2",
    "stripe": "^15.4.0",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^18.19.17",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.65",
    "@types/react-big-calendar": "^1.16.1",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "null-loader": "^4.0.1",
    "postcss": "^8.4.38",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5"
  }
}
