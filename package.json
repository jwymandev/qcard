{
  "name": "qcard",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.17.0"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "npx prisma generate && next build",
    "build:production": "NODE_ENV=production node scripts/db-migrate-production.js && npm run build",
    "deploy": "npx prisma migrate deploy && npm run build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "setup-uploads": "node scripts/init-uploads.js",
    "predev": "npm run setup-uploads",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run build",
    "find-case-issues": "grep -r \"prisma\\.\\([A-Z]\\)\" src/ --include=\"*.ts\" --include=\"*.tsx\"",
    "fix-schema": "node fix-schema.mjs --auto",
    "prevalidate": "npm run fix-schema",
    "check-auth": "node scripts/check-auth-config.js || true",
    "clean-build": "rm -rf .next && npm run build",
    "db:migrate": "node scripts/db-migrate-production.js",
    "db:studio": "prisma studio",
    "db:setup-postgres": "node prisma/setup-postgres.js",
    "db:setup-config": "node scripts/setup-db-config.js",
    "do:init-db": "node scripts/setup-db-config.js && node scripts/do-db-init.js",
    "do:deploy": "node scripts/setup-db-config.js && npm run do:init-db && npm run build",
    "do:build": "node scripts/setup-db-config.js && npx prisma generate && next build"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.0",
    "@prisma/client": "^5.14.0",
    "bcrypt": "^5.1.1",
    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.27",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^15.4.0",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^18.19.17",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "postcss": "^8.4.38",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5"
  }
}
